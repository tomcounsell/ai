{
  "workspace": {
    "name": "AI Rebuild Workspace",
    "version": "1.0.0",
    "description": "Comprehensive AI agent and tool management workspace",
    "created_at": "2025-01-01T00:00:00Z",
    "updated_at": "2025-01-01T00:00:00Z"
  },
  
  "agents": {
    "default_model": "claude-3-sonnet",
    "max_concurrent": 5,
    "timeout": 300,
    "retry_attempts": 3,
    "available_agents": [
      {
        "id": "claude-assistant",
        "name": "Claude Assistant",
        "model": "claude-3-sonnet",
        "description": "General purpose AI assistant",
        "capabilities": ["text_generation", "analysis", "reasoning", "coding"],
        "max_tokens": 4096,
        "temperature": 0.7,
        "enabled": true
      },
      {
        "id": "code-reviewer",
        "name": "Code Reviewer",
        "model": "claude-3-opus",
        "description": "Specialized code review and analysis agent",
        "capabilities": ["code_review", "security_analysis", "optimization"],
        "max_tokens": 8192,
        "temperature": 0.3,
        "enabled": true
      },
      {
        "id": "research-agent",
        "name": "Research Agent",
        "model": "claude-3-haiku",
        "description": "Fast research and information gathering",
        "capabilities": ["research", "summarization", "fact_checking"],
        "max_tokens": 2048,
        "temperature": 0.5,
        "enabled": true
      }
    ]
  },
  
  "tools": {
    "enabled": true,
    "auto_discovery": true,
    "tool_directories": [
      "tools/",
      "mcp_servers/",
      "integrations/"
    ],
    "available_tools": [
      {
        "id": "file-manager",
        "name": "File Manager",
        "type": "builtin",
        "description": "File system operations and management",
        "capabilities": ["read", "write", "list", "search"],
        "enabled": true,
        "config": {
          "max_file_size": "100MB",
          "allowed_extensions": ["*"],
          "restricted_paths": ["/etc", "/sys", "/proc"]
        }
      },
      {
        "id": "web-search",
        "name": "Web Search",
        "type": "api",
        "description": "Internet search capabilities",
        "capabilities": ["search", "summarize"],
        "enabled": true,
        "config": {
          "provider": "perplexity",
          "max_results": 10,
          "timeout": 30
        }
      },
      {
        "id": "code-executor",
        "name": "Code Executor",
        "type": "sandbox",
        "description": "Safe code execution environment",
        "capabilities": ["python", "javascript", "shell"],
        "enabled": false,
        "config": {
          "timeout": 60,
          "memory_limit": "512MB",
          "network_access": false
        }
      },
      {
        "id": "telegram-bot",
        "name": "Telegram Integration",
        "type": "integration",
        "description": "Telegram bot and messaging integration",
        "capabilities": ["send_message", "receive_message", "file_upload"],
        "enabled": false,
        "config": {
          "webhook_url": null,
          "polling_interval": 1000,
          "max_message_length": 4096
        }
      },
      {
        "id": "notion-sync",
        "name": "Notion Integration",
        "type": "integration",
        "description": "Notion database and page synchronization",
        "capabilities": ["read_database", "write_page", "sync"],
        "enabled": false,
        "config": {
          "default_database_id": null,
          "sync_interval": 3600,
          "auto_create_pages": true
        }
      }
    ]
  },
  
  "workflows": {
    "enabled": true,
    "auto_save": true,
    "max_concurrent": 3,
    "default_timeout": 600,
    "templates": [
      {
        "id": "code-review-workflow",
        "name": "Code Review Workflow",
        "description": "Automated code review process",
        "steps": [
          {
            "id": "analyze",
            "agent": "code-reviewer",
            "action": "analyze_code",
            "timeout": 120
          },
          {
            "id": "security-check",
            "agent": "code-reviewer", 
            "action": "security_analysis",
            "timeout": 60
          },
          {
            "id": "generate-report",
            "agent": "claude-assistant",
            "action": "create_report",
            "timeout": 30
          }
        ],
        "enabled": true
      },
      {
        "id": "research-workflow",
        "name": "Research Workflow",
        "description": "Information gathering and analysis process",
        "steps": [
          {
            "id": "search",
            "tool": "web-search",
            "action": "search",
            "timeout": 30
          },
          {
            "id": "analyze",
            "agent": "research-agent",
            "action": "analyze_results",
            "timeout": 60
          },
          {
            "id": "summarize",
            "agent": "claude-assistant",
            "action": "create_summary",
            "timeout": 30
          }
        ],
        "enabled": true
      }
    ]
  },
  
  "integrations": {
    "telegram": {
      "enabled": false,
      "bot_token": null,
      "webhook_url": null,
      "allowed_users": [],
      "commands": {
        "/help": "Show available commands",
        "/status": "Get system status",
        "/chat": "Start AI chat session",
        "/stop": "Stop current session"
      }
    },
    
    "notion": {
      "enabled": false,
      "workspace_id": null,
      "default_database": null,
      "sync_settings": {
        "auto_sync": false,
        "sync_interval": 3600,
        "conflict_resolution": "latest_wins"
      }
    },
    
    "mcp": {
      "enabled": true,
      "server_discovery": true,
      "auto_connect": false,
      "servers": [
        {
          "name": "filesystem-server",
          "command": "npx",
          "args": ["-y", "@modelcontextprotocol/server-filesystem"],
          "env": {
            "ALLOWED_DIRECTORIES": "."
          },
          "enabled": true
        },
        {
          "name": "web-search-server",
          "command": "npx",
          "args": ["-y", "@modelcontextprotocol/server-web-search"],
          "env": {},
          "enabled": false
        }
      ]
    }
  },
  
  "security": {
    "sandbox_mode": true,
    "allowed_domains": [
      "localhost",
      "127.0.0.1",
      "*.anthropic.com",
      "*.openai.com",
      "*.perplexity.ai",
      "*.notion.com",
      "*.telegram.org"
    ],
    "blocked_commands": [
      "rm -rf",
      "sudo",
      "su",
      "chmod 777",
      "chown",
      "mkfs",
      "dd"
    ],
    "file_restrictions": {
      "max_upload_size": "100MB",
      "allowed_extensions": [
        ".txt", ".md", ".py", ".js", ".json", ".yaml", ".yml",
        ".csv", ".tsv", ".log", ".conf", ".cfg", ".ini"
      ],
      "blocked_paths": [
        "/etc/passwd",
        "/etc/shadow",
        "/etc/hosts",
        "/root",
        "/sys",
        "/proc"
      ]
    }
  },
  
  "database": {
    "path": "data/ai_rebuild.db",
    "backup_on_startup": true,
    "run_migrations": true,
    "pool_size": 5
  },
  
  "components": {
    "agents": true,
    "mcp_servers": true,
    "telegram": false,
    "tools": true
  },
  
  "monitoring": {
    "enabled": true,
    "dashboard_port": 8081,
    "dashboard_host": "localhost",
    "log_level": "INFO",
    "metrics": {
      "collect_performance": true,
      "collect_usage": true,
      "retention_days": 30
    },
    "alerts": {
      "email_enabled": false,
      "webhook_enabled": false,
      "thresholds": {
        "memory_usage": 80,
        "cpu_usage": 90,
        "error_rate": 5,
        "response_time": 5000
      }
    }
  },
  
  "backup": {
    "enabled": true,
    "auto_backup": true,
    "backup_interval": 86400,
    "retention_count": 7,
    "backup_locations": [
      "data/backups/"
    ],
    "include_files": [
      "config/",
      "data/",
      "logs/"
    ],
    "exclude_patterns": [
      "*.tmp",
      "*.log",
      "temp/",
      ".env"
    ]
  }
}